    "def f(x):\n",
    "    return ord(x) - 97\n",
    "\n",
    "def g(x):\n",
    "    return x + 3 % 26\n",
    "\n",
    "def h(x):\n",
    "    return chr(x + 97)\n",
    "\n",
    "def ceaserEncrypt(x):\n",
    "    result = \"\"\n",
    "    for c in list(x):\n",
    "        result += h(g(f(c)))\n",
    "    return result\n",
    "\n",
    "ceaserEncrypt('test')"
   ]
